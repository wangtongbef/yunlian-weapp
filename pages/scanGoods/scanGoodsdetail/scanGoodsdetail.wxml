<!--pages/scanGoods/scanGoodsdetail/scanGoodsdetail.wxml-->
<view class='detailtop'>
  <view class='productType'>产品型号：{{Type}}</view>
  <view class='productName'>产品名称：{{name}}</view>
</view>
<view class='detailbottom'>
  <view class='bottomtitle'>产品编号</view>
  <view wx:if='{{product_ids.length!=0}}' class='productids'>
    <block wx:for='{{product_ids}}' wx:key="{{index}}">
      <view class='productid'>
        {{item}}
      </view>
    </block>
  </view>
  <view wx:if='{{product_ids.length==0}}' class='productids'>
    <view class='productid'>
      {{options.code_sn}}
    </view>
  </view>
</view>