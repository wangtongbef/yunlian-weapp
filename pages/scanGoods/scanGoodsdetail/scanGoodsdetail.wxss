/* pages/scanGoods/scanGoodsdetail/scanGoodsdetail.wxss */
page{
  background-color: rgb(248, 248, 248)
}
.detailtop{
  height:225rpx;
  width: 100%;
  background-color: #fff;
}
.productType,
.productName{
  position: absolute;
  left: 40rpx;
  color: rgb(88, 88, 88);
  font-size: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
  width: calc(100% - 80rpx);
}
.productType{
  top:60rpx
}
.productName{
  top:132rpx
}
.detailbottom{
  margin-top: 20rpx;
  background-color: #fff;
}
.bottomtitle{
  width: 100%;
  height: 92rpx;
  color: rgb(88, 88, 88);
  font-size: 32rpx;
  text-align: center;
  line-height: 112rpx;
  border-bottom: 2rpx solid rgb(238, 238, 238)
}
.productid{
  width:calc(100% - 35rpx);
  margin-left: 35rpx;
  height:96rpx;
  letter-spacing: 35rpx;
  text-align: center;
  line-height: 156rpx;
  color: rgb(42, 41, 41);
  font-size: 36rpx;
}
.productids .productid:last-child{
  padding-bottom: 40rpx;
}