/* pages/goodsSend/goodssendDetail/goodssendDetail.wxss */
page{
  background-color: rgb(238,238,238);
  height:100%
}
.title{
  height:55rpx;
  width: 100%;
  padding:45rpx 0 0 40rpx;
  font-size: 28rpx;
  color: rgb(167,167,167)
}
.content{
  background-color: #fff;
  padding:40rpx 0;
}
.content view{
  height:70rpx;
  width: calc(100% - 80rpx);
  padding: 0 40rpx;
  display: flex;
  justify-content: space-between;
  align-items: center
}
.ct{
  font-size: 28rpx;
  color:rgb(88,88,88);
}
.cn{
  font-size: 28rpx;
  color:rgb(42,41,41);
}
.cc{
  font-size: 32rpx;
  color:rgb(42,41,41);
}
.listBox{
  width:100%;
  padding-top:50rpx;
  background-color: #fff;
}
.list{
  width:calc(100% - 80rpx);
  height:36rpx;
  margin:0 40rpx 0;
  padding-bottom: 40rpx;
  display:flex;
  justify-content: flex-end;
  align-items: flex-end;
}
.listBox .list:last-child{
  padding-bottom: 65rpx;
}
.productName{
  width: 320rpx;
  font-size: 28rpx;
  color: rgb(88, 88, 88);
  height: 28rpx
}
.productAmount,.productMeasure{
  text-align: right;
  color: rgb(42, 41, 41);
}
.productAmount{
  width: 320rpx;
  font-size: 36rpx;
  height: 36rpx
}
.productMeasure{
  width: 30rpx;
  font-size: 20rpx;
  height: 20rpx
}
.mb118{
  margin-bottom: 118rpx;
}
.returnButton{
  width:100%;
  height:118rpx;
  font-size: 40rpx;
  position: fixed;
  display: flex;
  align-items: center;
  bottom: 0
}
.returnButton-1{
  background-color: rgb(1,144,210);
  justify-content: center;
  color: #fff;
}
.clear,
.send,
.stockUp{
  width:50%;
  height:118rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.clear{
  background-color: rgb(248,248,248);
  color: rgb(1,144,210)
}
.send,
.stockUp{
  background-color: rgb(1,144,210);
  color: #fff;
}