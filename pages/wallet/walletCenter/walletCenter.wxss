/* pages/wallet/takeCash/takeCash.wxss */
page{
  background: #eee;
}
.background{
  background: linear-gradient(#018fe8,#0072b1);
  color: #fff;
}
.flex{
  display: flex;
  justify-content: space-between
}
.width{
  width: 90%;
  padding: 5%
}
.takeCashInfo{
  font-size: 30rpx;
}
.takeCashInfo image{
  width: 40rpx;
  height: 40rpx;
}
.money{
  font-size: 80rpx;
  text-align: center;
  margin: 80rpx 0;
}
.nearby .title{
  font-size: 30rpx;
  padding: 10rpx 5%;
  width: 90%
}
.nearby .detail{
  background: #fff;
  width: 90%;
  padding: 16rpx 5%;
  font-size: 30rpx;
  align-items: flex-end;
  border-bottom: 1rpx #eee solid
}
.left text:last-child{
  display: block
}
.moneyType{
  font-size: 30rpx;
  font-weight: bold;

}
.red{
  color: #9E0000
}
.gray{
  color: gray
}
.takeCashBtn{
  width: 100%;
  position: fixed;
  bottom: 140rpx;
}
.takeCashBtn button{
  width: 90%;
  margin: 0 auto;
}